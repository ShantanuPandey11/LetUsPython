#armstrong number 1-500

for i in range(1,10000):
    temp = i
    times=0
    Sum=0
    while temp>0:
        times=times+1
        temp=temp//10
        #print('w',temp,times)
        
    temp = i
    
    while temp>0:
        reminder = temp%10
        Sum+=reminder**times
        #print('i:',i,'\t',Sum,'r',reminder,'ti',times)
        temp//=10
        if Sum==i:
            print(i,'yes',Sum)
            break 
